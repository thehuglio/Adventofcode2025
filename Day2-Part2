package org.thehuglio;
import java.io.File;
import java.util.*;

public class Main {

    private static final List<String> data = new Reader(new File("data.txt")).data;
    public static void main(String[] args) {
        System.out.println(data);
        long invalid = 0;
        for (String range : data.get(0).split(",")) {
            long num = Long.parseLong(range.split("-")[0]);
            long end = Long.parseLong(range.split("-")[1]);
            while (num <= end) {
                if (!isvalid(num)) invalid += num;
                num++;
            }
        }
        System.out.println(invalid);
    }
    public static Boolean isvalid(long num) {
        String numstring = String.valueOf((long) num);
        String[] split = numstring.split("");
        for (int i = 2; i <= split.length; i++) {
            if (hasrepeating(split,i)) return false;
        }
        return true;
    }
    public static Boolean hasrepeating(String[] s, int dev) {
        if (s.length % dev == 0) {
            int partlenght = s.length/dev;
            for (int i = 0; i < dev; i++) {
                for (int j = 0; j < partlenght; j++) {
                    if (!s[j].equals(s[j + partlenght*i])) return false;
                }
            }
            return true;
        }
        else return false;
    }
}
